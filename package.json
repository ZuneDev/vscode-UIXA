{
  "name": "uixa",
  "displayName": "Iris UI",
  "description": "Provides language support for UIX Assembly and Scripts",
  "publisher": "YoshiAsk",
  "repository": {
    "url": "https://github.com/ZuneDev/vscode-UIXA",
    "type": "github"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "uixa",
        "aliases": ["UIX Assembly", "uixa"],
        "extensions": [".uixa"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "uix-script",
        "aliases": ["UIX Script", "uix-script"],
        "extensions": [".uix-script"],
        "configuration": "./uix-script_language-configuration.json"
      },
      {
        "id": "uix-xml",
        "aliases": ["UIX XML", "uix-xml"],
        "extensions": [".uix"]
      }
    ],
    "grammars": [
      {
        "language": "uixa",
        "scopeName": "source.uixa",
        "path": "./syntaxes/UIXA.tmLanguage"
      },
      {
        "language": "uix-script",
        "scopeName": "source.uix-script",
        "path": "./syntaxes/uix-script.tmLanguage.json"
      },
      {
        "language": "uix-xml",
        "scopeName": "source.uix-xml",
        "path": "./syntaxes/uix-xml.tmLanguage.json"
      }
    ]
  }
}
